                        "join": 1,
                        "message": 1,
                        "mod": 1,
                        "mods": 1,
                        "notice": 1,
                        "part": 1,
                        "r9kbeta": 1,
                        "resub": 1,
                        "roomstate": 1,
                        "serverchange": 1,
                        "slowmode": 1,
                        "subscribers": 1,
                        "subscription": 1,
                        "timeout": 1,
                        "unhost": 1,
                        "unmod": 1
                    }
                },
                "reason": {
                    "id": "reason",
                    "position": {
