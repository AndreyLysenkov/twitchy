                "disconnected",
                "hosting",
                "hosted",
                "logon",
                "timeout",
                "unhost"
            ],
            "require": "./{path}.js",
            "id": {
                "unsupported": {
                    "path": "unsupported",
                    "json_space": 4,
                    "template": "[{time}] > **unsupported event** \n```json\n{stringified}\n```"
                },
                "action": {
                    "path": "main",
