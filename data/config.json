{
    "log": {
        "level": {
            "verbose": 0,
            "debug": 1,
            "main": 2,
            "warn": 3,
            "error": 4
        },
        "receiver": {
            "list": [
                "console"
            ],
            "console": {
                "template": {
                    "main": {
                        "content": "[{time_formatted}] > [{module}.{level}] > {message}",
                        "line": " >> {line}",
                        "skip": 1,
                        "split": "\n"
                    },
                    "error": {
                        "content": "[{time_formatted}] > [{module}.{level}] > {message} >\n{error_formatted}",
                        "line": " >> {line}",
                        "skip": 1,
                        "split": "\n",
                        "spaces": 4
                    }
                },
                "time_format": "YYYY-MM-DD>HH:mm:ss.SSS",
                "style": {
                    "verbose": [
                        "bgBlue",
                        "black"
                    ],
                    "debug": [
                        "bgBlue",
                        "white"
                    ],
                    "main": [
                        "bgWhite",
                        "black"
                    ],
                    "warn": [
                        "bgYellow",
                        "black"
                    ],
                    "error": [
                        "bgRed",
                        "white"
                    ]
                }
            }
        },
        "list": [
            {
                "type": "console",
                "levels": [
                    "verbose",
                    "debug",
                    "main",
                    "warn",
                    "error"
                ]
            }
        ],
        "module": "app.main"
    },
    "discord": {
        "log": {
            "list": [
                {
                    "event": "debug",
                    "level": "verbose"
                },
                {
                    "event": "warn",
                    "level": "warn"
                },
                {
                    "event": "error",
                    "level": "error"
                }
            ],
            "module": "app.discord"
        },
        "guild": {
            "file": {
                "name": "config.{id}.json",
                "options": {
                    "onCreate": {
                        "encoding": "utf8",
                        "flag": "w"
                    },
                    "onRewrite": {
                        "encoding": "utf8"
                    }
                },
                "spaces": 4,
                "content": {
                    "channels": []
                }
            }
        },
        "chat": {
            "limit": 1800,
            "template": "{line}"
        },
        "regex": "\\[*`~_]\\g"
    },
    "twitch": {
        "client": {
            "options": {
                "debug": true
            },
            "connection": {
                "port": 443,
                "reconnect": true,
                "maxReconnectAttempts": 20,
                "maxReconnectInterval": 30000,
                "reconnectDecay": 1.75,
                "reconnectInterval": 1000,
                "secure": true,
                "timeout": 5000
            }
        },
        "log": {
            "list": [
                {
                    "event": "info",
                    "level": "verbose"
                },
                {
                    "event": "warn",
                    "level": "warn"
                },
                {
                    "event": "error",
                    "level": "error"
                }
            ],
            "module": "app.twitch"
        },
        "channels": [
            "#status#",
            "#allan_walpy",
            "#jackshepardtwitch"
        ],
        "status": "#status#",
        "event": [
            {
                "name": "action",
                "enable": true
            },
            {
