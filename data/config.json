    "stringify": {
        "event": {
            "list": [
                "action",
                "ban",
                "chat",
                "cheer",
                "clearchat",
                "connecting",
                "connected",
                "disconnected",
                "hosting",
                "hosted",
                "logon",
                "timeout",
                "unhost",
                "emoteonly",
                "followersonly",
                "join",
                "part",
                "message",
                "mod",
                "resub",
                "subscription",
                "ping",
                "pong",
                "r9kbeta",
                "reconnect",
                "slowmode",
                "serverchange",
                "subscribers",
                "unmod",
                "whisper",
