{
    "name": "twitchy",
    "version": "0.6.1-beta",
    "license": "MIT",
    "description": "log twitch chat to discord chat",
    "keywords": [
        "discord",
        "twitch",
        "twitch-chat",
        "twitch-log",
        "bot"
    ],
    "author": {
        "name": "allan_walpy#3408 aka andrey lysenkov",
        "email": "andreylysenkovallan@gmail.com",
        "url": "https://allan-walpy.github.io"
    },
    "homepage": "https://github.com/allan-walpy/twitchy#readme",
    "repository": "github:allan_walpy/twitchy",
    "bugs": {
        "url": "https://github.com/allan-walpy/twitchy/issues"
    },
    "directories.lib": "/src",
    "config": {
        "data-directory": "data",
        "guild-config-directory": "guild",
        "lang-file": "lang.json",
        "config-file": "config.json",
        "private-config-file": "private.json"
    },
    "os": [
        "linux",
        "win32",
        "darwin"
    ],
    "engines": {
        "node": ">=8.11.0",
        "npm": ">=5.6.0"
    },
    "dependencies": {
        "discord.js": "github:discordjs/discord.js",
        "tmi.js": "github:tmijs/tmi.js",
        "string-format": "^2.0.0",
        "moment": "^2.22.1",
        "moment-timezone": "^0.5.17",
        "chalk": "^2.4.1"
    },
    "devDependencies": {
        "eslint": "github:eslint/eslint"
    },
    "bundleDependencies": [
        "discord.js",
        "tmi.js",
        "string-format",
        "moment",
        "moment-timezone",
        "chalk"
    ],
    "main": "src/main.js",
    "scripts": {
        "start": "npm run-script start-win || npm run-script start-linux",
        "start-win": "node --use-strict ./src/main.js",
        "start-linux": "nodejs --use-strict ./src/main.js"
    }
}
