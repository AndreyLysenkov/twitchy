    "stringify": {
        "event": {
            "list": [
                "action",
                "chat"
            ],
            "require": "./{path}.js",
            "id": {
                "unsupported": {
                    "path": "unsupported",
                    "json_space": 4,
                    "template": "**unsupported event** \n```json\n{stringified}\n```"
                },
                "action" : {
                    "path": "message",
                    "badges": {
                        "separator": ", "
                    },
                    "username": {
                        "template": "{name} [{display}]"
                    },
                    "template": "[{time}] {badges} > **{username} {message}**",
                    "time": "HH:mm:ss"
                },
                "chat" : {
                    "path": "message",
                    "badges": {
                        "separator": ", "
                    },
                    "username": {
                        "template": "{name} [{display}]"
                    },
                    "template": "[{time}] {badges} > **{username}** : {message}",
                    "time": "HH:mm:ss"
                }
            }
        }
    }
}